<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net7.0;net48</TargetFrameworks>
    <OutputType>Library</OutputType>
    <RootNamespace>Pachyderm_Acoustic</RootNamespace>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <!--<UseWPF>false</UseWPF>-->
